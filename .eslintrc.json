{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.config.babel.js"
        }
      }
    },
    "rules": {
      "semi": 0,
      "no-multi-spaces": 0,
      "camelcase": 0,
      "consistent-return": 0,
      "no-var": 0,
      "quotes": 0,
      "no-use-before-define": 0,
      "class-methods-use-this": 0,
      "eqeqeq": 0,
      "react/no-did-mount-set-state": 0,
      "react/require-default-props": 1,
      "react/prop-types": 1
    }
}